unit Model.Clientes;

interface

uses
  Spring,
  Spring.Persistence.Mapping.Attributes;

type
  [Entity]
  [Table('Cliente')]
    private
     [AutoGenerated]
     FID_Cliente: integer;

     procedure SetID_Cliente(const Value: integer);

    public
       property ID_Cliente: Integer read FID_Cliente write SetID_Cliente;
  end;

implementation

end.
